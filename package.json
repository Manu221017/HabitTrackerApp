{
  "name": "habittrackerapp",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "powershell -ExecutionPolicy Bypass -File ./start-expo.ps1",
    "start:original": "npx expo start --dev-client",
    "start:tunnel": "npx expo start --dev-client --tunnel --clear",
    "start:tunnel:retry": "npx expo start --dev-client --tunnel",
    "start:lan": "powershell -ExecutionPolicy Bypass -File ./start-without-tunnel.ps1",
    "setup:ngrok": "powershell -ExecutionPolicy Bypass -File ./configurar-ngrok-simple.ps1",
    "check:ngrok": "powershell -ExecutionPolicy Bypass -File ./check-ngrok.ps1",
    "fix:ngrok": "powershell -ExecutionPolicy Bypass -File ./solucionar-ngrok-definitivo.ps1",
    "diagnostico:red": "powershell -ExecutionPolicy Bypass -File ./diagnostico-red.ps1",
    "fix:deps": "powershell -ExecutionPolicy Bypass -File ./fix-dependencies.ps1",
    "arreglar:ngrok": "powershell -ExecutionPolicy Bypass -File ./arreglar-ngrok-config.ps1",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "test": "jest"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/ngrok": "^4.1.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/native-stack": "^7.3.23",
    "@react-navigation/stack": "^7.4.5",
    "expo": "^54.0.7",
    "expo-calendar": "~15.0.7",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.7",
    "expo-notifications": "~0.32.11",
    "expo-status-bar": "~3.0.8",
    "firebase": "^12.0.0",
    "ngrok": "^5.0.0-beta.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.21.0"
