  "devDependencies": {
    "@babel/core": "^7.20.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.17"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "private": true
